   "source": [
    "import snowflake.connector\n",
    "from tabulate import tabulate\n",
    "from langchain.llms import OpenAI\n",
    "import openai\n",
    "import os\n",
    "import json\n",
    "import pandas as pd\n",
    "\n",
    "openai.api_key = \"abc-12\"\n",
    "\n",
    "def get_completion(prompt, model=\"gpt-4\", seed=None):\n",
    "    messages = [{\"role\": \"system\", \"content\": prompt}]\n",
    "    response = openai.ChatCompletion.create(\n",
    "        model=model,\n",
    "        messages=messages,\n",
    "        temperature=0.2,\n",
    "        seed=seed,\n",
    "        # response_format={ \"type\": \"json_object\" }\n",
    "    )\n",
    "    return response.choices[0].message[\"content\"]\n",
    "\n",
    "snowflake_account = ''\n",
    "snowflake_user = ''\n",
    "snowflake_password = ''\n",
    "snowflake_warehouse = ''\n",
    "snowflake_database = ''\n",
    "snowflake_schema = 'DATA_S3'\n",
    "\n",
    "conn = snowflake.connector.connect(\n",
    "    user=snowflake_user,\n",
    "    password=snowflake_password,\n",
    "    account=snowflake_account,\n",
    "    warehouse=snowflake_warehouse,\n",
    "    database=snowflake_database,\n",
    "    schema=snowflake_schema\n",
    ")\n",
    "\n",
    "NO_of_unique_samples = 5\n",
    "\n",
    "cursor = conn.cursor()\n",
    "\n",
    "if NO_of_unique_samples <= 5:\n",
    "    table_query = f\"\"\" SELECT  NONNUMERICPART as remove_this_part, \n",
    "                                 REVENUEDETAILS_REVENUE as sample_records, \n",
    "                                 NumericPart as after_conversion\n",
    "                     FROM (\n",
    "                          SELECT *,\n",
    "                                 row_number() OVER (PARTITION BY NonNumericPart ORDER BY rn) AS sample_count_filter,\n",
    "                                 row_number() OVER (PARTITION BY NonNumericPart, to_be_changed ORDER BY rn) AS unique_sample_count_filter\n",
    "                          FROM (\n",
    "                              SELECT REVENUEDETAILS_REVENUE,\n",
    "                                     CASE WHEN TRY_CAST(REVENUEDETAILS_REVENUE AS NUMBER) IS NULL THEN 'yes' ELSE 'no' END AS change_required,\n",
    "                                     TRY_CAST(REGEXP_REPLACE(REVENUEDETAILS_REVENUE, '[^0-9.]', '') AS DECIMAL(18,4)) AS NumericPart,\n",
    "                                     REGEXP_REPLACE(REVENUEDETAILS_REVENUE, '[0-9.]', '') AS NonNumericPart,\n",
    "                                     CASE WHEN TRY_CAST(REVENUEDETAILS_REVENUE AS NUMBER) IS NULL THEN REVENUEDETAILS_REVENUE ELSE NULL END AS to_be_changed,\n",
    "                                      ROW_NUMBER () OVER (ORDER BY REVENUEDETAILS_REVENUE) AS rn\n",
    "                              FROM INTERMEDIA.DATA_S3.REVENUE\n",
    "                          )\n",
    "                      )\n",
    "                      WHERE TO_BE_CHANGED IS NOT NULL AND \n",
    "                            sample_count_filter <= 5 AND \n",
    "                            unique_sample_count_filter = 1  \"\"\"\n",
    "\n",
    "    cursor.execute(table_query)\n",
    "    result = cursor.fetchall()\n",
    "\n",
    "    headers = [\"remove_this_part\", \"sample_records\", \"after_conversion\"]\n",
    "    print(tabulate(result, headers=headers, tablefmt=\"grid\"))\n",
    "else:\n",
    "    table_query = f\"\"\"SELECT  distinct\n",
    "                                 REVENUEDETAILS_REVENUE as sample_records\n",
    "                     FROM (\n",
    "                          SELECT *,\n",
    "                                 row_number() OVER (PARTITION BY NonNumericPart ORDER BY rn) AS sample_count_filter,\n",
    "                                 row_number() OVER (PARTITION BY NonNumericPart, to_be_changed ORDER BY rn) AS unique_sample_count_filter\n",
    "                          FROM (\n",
    "                              SELECT REVENUEDETAILS_REVENUE,\n",
    "                                     CASE WHEN TRY_CAST(REVENUEDETAILS_REVENUE AS NUMBER) IS NULL THEN 'yes' ELSE 'no' END AS change_required,\n",
    "                                     TRY_CAST(REGEXP_REPLACE(REVENUEDETAILS_REVENUE, '[^0-9.]', '') AS DECIMAL(18,4)) AS NumericPart,\n",
    "                                     REGEXP_REPLACE(REVENUEDETAILS_REVENUE, '[0-9.]', '') AS NonNumericPart,\n",
    "                                     CASE WHEN TRY_CAST(REVENUEDETAILS_REVENUE AS NUMBER) IS NULL THEN REVENUEDETAILS_REVENUE ELSE NULL END AS to_be_changed,\n",
    "                                      ROW_NUMBER () OVER (ORDER BY REVENUEDETAILS_REVENUE) AS rn\n",
    "                              FROM INTERMEDIA.DATA_S3.REVENUE\n",
    "                          )\n",
    "                      )\n",
    "                      WHERE TO_BE_CHANGED IS NOT NULL AND \n",
    "                            sample_count_filter <= 5 AND \n",
    "                            unique_sample_count_filter = 1  \"\"\"\n",
    "\n",
    "    cursor.execute(table_query)\n",
    "    result = cursor.fetchall()\n",
    "\n",
    "    data_list = [row[0] for row in result]\n",
    "    print(data_list)\n",
    "\n",
    "    # Billing\n",
    "    prompt = f\"\"\"\n",
    "        I want to make a bot which says to the user which is dirty data to be converted into the revenue field.\n",
    "        All dirty data would not be of the same type, so different actions have to be taken. So, group data as per similarities.\n",
    "\n",
    "        Similar groups would be based on Positive Values, Negative Values, Empty Values, String Values, Mixed Values, k based value & others\n",
    "\n",
    "        1. Positive Values - consist of numeric amounts accompanied by a currency symbol or every three digits separated by a comma representing thousands separators for the amount. eg (5,431,321 and not 5,42,11,2) or both dollar & comma\n",
    "        2. Negative Values\n",
    "        3. Empty Values - contains symbol data\n",
    "        4. String Values - contains non-numeric data\n",
    "        5. Mixed Values - consists of both numeric & non-numeric data\n",
    "        6. k based value - Values beginning with a number and ending with k (e.g., 900k, 123k, 55k)\n",
    "        7. others - If you are not able to group based on similarities then add it in others\n",
    "\n",
    "        I have values in [{data_list}].\n",
    "\n",
    "        Give Output in json format without explanation \n",
    "    \"\"\"\n",
    "\n",
    "    response = get_completion(prompt)\n",
    "    print(response)\n",
    "    \n",
    "    \n",
    "    json_data = json.loads(response)\n",
    "    headers = [\"Category\", \"Values\"]\n",
    "    rows = []\n",
    "    for category, values in json_data.items():\n",
    "        values_subset = values[:]\n",
    "        for value in values_subset:\n",
    "            rows.append([category, value])\n",
    "    df = pd.DataFrame(rows, columns=headers)\n",
    "    print(df)\n",
    "\n",
    "\n",
    "    # Drop the existing table\n",
    "    drop_table_query = \"\"\"\n",
    "        DROP TABLE IF EXISTS DATA_S3.REVENUE_SUGGESTIONS;\n",
    "    \"\"\"\n",
    "    cursor.execute(drop_table_query)\n",
    "\n",
    "    # Your SQL query\n",
    "    specific_string = \"$\"\n",
    "    table_query = f\"\"\"\n",
    "        SELECT\n",
    "        REVENUEDETAILS_REVENUE\n",
    "        FROM INTERMEDIA.DATA_S3.REVENUE\n",
    "    \"\"\"\n",
    "\n",
    "    cursor.execute(table_query)\n",
    "    result = cursor.fetchall()\n",
    "\n",
    "    df_revenue_sample = pd.DataFrame(result, columns=[\"REVENUEDETAILS_REVENUE\"])\n",
    "\n",
    "    result_df = pd.merge(df, df_revenue_sample, how='inner', left_on='Values', right_on='REVENUEDETAILS_REVENUE')\n",
    "\n",
    "    # Drop duplicates based on the 'REVENUEDETAILS_REVENUE' column\n",
    "    result_df = result_df.drop_duplicates(subset=['REVENUEDETAILS_REVENUE'])\n",
    "\n",
    "    # Creating the table\n",
    "    create_table_query = \"\"\"\n",
    "        CREATE TABLE IF NOT EXISTS DATA_S3.REVENUE_SUGGESTIONS (\n",
    "            CATEGORY STRING,\n",
    "            raw_revenue_data STRING\n",
    "        );\n",
    "    \"\"\"\n",
    "    cursor.execute(create_table_query)\n",
    "    \n",
    "    drop_table_query_final = \"\"\"\n",
    "    DROP TABLE IF EXISTS DATA_S3.REVENUE_SUGGESTIONS_final;\n",
    "    \"\"\"\n",
    "    cursor.execute(drop_table_query_final)\n",
    "\n",
    "    # Create the final table\n",
    "    fill_table_query = \"\"\"\n",
    "    CREATE TABLE IF NOT EXISTS DATA_S3.REVENUE_SUGGESTIONS_final AS  (\n",
    "    SELECT a.REVENUEDETAILS_REVENUE, b.*\n",
    "        FROM DATA_S3.REVENUE a\n",
    "        LEFT JOIN DATA_S3.REVENUE_SUGGESTIONS b\n",
    "        ON REGEXP_REPLACE(a.REVENUEDETAILS_REVENUE, '[0-9.]', '') = REGEXP_REPLACE(b.raw_revenue_data, '[0-9.]', '')\n",
    "    ) \n",
    "    \"\"\"\n",
    "    cursor.execute(fill_table_query)\n",
    "\n",
    "\n",
    "cursor.close()\n",
    "conn.close()\n"
   ]
  },
